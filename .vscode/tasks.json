{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/EHsc",
                "/Iinclude",
                "/Zi",
                "/Od",
                "/MDd",
                "/D DEBUG",
                "/Fobuild/",
                "/Fdbuild/vc140.pdb",
                "/Febuild/${fileBasenameNoExtension}.exe",
                "src/${fileBasenameNoExtension}.cpp",
                "/link",
                "/LIBPATH:lib\\Debug",
                "/NODEFAULTLIB:LIBCMT",
                "/NODEFAULTLIB:libcmt",
                "scamp5_sim_lib.lib",
                 "Kernel32.lib",
                 "Advapi32.lib",
                 "User32.lib"

            ],
            "options": {
                "cwd": "${fileDirname}/../"
               
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}